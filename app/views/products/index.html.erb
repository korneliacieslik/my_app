<p id="notice"><%= notice %></p>

<h1>Products</h1>
<table>
    <thead>
      <tr>
        <th>Description</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
      <tbody>
        <% @products.each do |product| %>
          <tr>
            <td><%= product.description %></td>
            <td><%= product.name %></td>
            <td><%= link_to 'Show', product %></td>
          </tr>
        <% end %>
      </tbody>
    </thead>
  </table>